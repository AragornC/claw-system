# 必填：你的 DeepSeek API key
DEEPSEEK_API_KEY=sk-xxxx

# 对外端口（腾讯云安全组需放行）
PUBLIC_PORT=8765

# OpenClaw agent 配置
OPENCLAW_AGENT_LOCAL=1
OPENCLAW_AGENT_ID=main
OPENCLAW_TIMEOUT_SEC=90
OPENCLAW_CHAT_TIMEOUT_MS=95000

# 启动时是否刷新 report 数据（1=刷新）
REFRESH_REPORT_DATA=0
REPORT_MAX_DECISIONS=400

# 时区
TZ=Asia/Shanghai
