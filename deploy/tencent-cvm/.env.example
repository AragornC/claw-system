# 可选：DeepSeek API key（若使用 deepseek provider 则填写）
DEEPSEEK_API_KEY=sk-xxxx

# 对外端口（腾讯云安全组需放行）
PUBLIC_PORT=8765

# OpenClaw agent 配置
OPENCLAW_AGENT_LOCAL=1
OPENCLAW_AGENT_ID=main
OPENCLAW_CHANNEL=
OPENCLAW_TO=
OPENCLAW_SESSION_ID=
OPENCLAW_DELIVER=0
OPENCLAW_REPLY_CHANNEL=
OPENCLAW_REPLY_TO=
OPENCLAW_REPLY_ACCOUNT=
OPENCLAW_THINKING=medium
OPENCLAW_VERBOSE=off
OPENCLAW_TIMEOUT_SEC=90
OPENCLAW_CHAT_TIMEOUT_MS=95000

# 模型默认项（启动容器时写入 openclaw config）
OPENCLAW_PRIMARY_MODEL=deepseek/deepseek-chat
OPENCLAW_BOOTSTRAP_DEEPSEEK=1

# 启动时是否刷新 report 数据（1=刷新）
REFRESH_REPORT_DATA=0
REPORT_MAX_DECISIONS=400

# 时区
TZ=Asia/Shanghai
